#!/bin/sh
. "$(dirname "$0")/_/husky.sh"

green='\e[1;32m'
blue='\e[1;34m'
cyan='\e[1;36m'
resetColor='\e[0m'

printf "$green-------- Start Run Format ---------$resetColor" + '\n'
npm run format
printf "$green-------- End Run Format ---------$resetColor" + '\n'

git add -A .
printf "\n \n $cyan-------- Complete --------->$resetColor" + '\n \n'
echo ""
echo "\n"
